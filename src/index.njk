<div>
  <ul class="post-list">
  {%- for post in collections.post | reverse  -%}
    <li>
      <div class="post-title">
        <a class="title" href="{{post.url}}">{{ post.data.title }}</a>
        <div class="date">{{ post.date | readableDate }}</div>
      </div>
    </li>
  {%- endfor -%}
  </ul>
</div>